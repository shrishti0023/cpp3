#include <iostream>
using namespace std;

int main() {
    int num, octal[32], i = 0;

    cout << "Enter decimal number: ";
    cin >> num;

    while(num > 0) {
        octal[i] = num % 8;
        num /= 8;
        i++;
    }

    cout << "Octal = ";
    for(int j = i - 1; j >= 0; j--) {
        cout << octal[j];
    }

    return 0;
}
