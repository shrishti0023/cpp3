#include <iostream>
using namespace std;

int main() {
    int num, digit, target, count = 0;

    cout << "Enter number: ";
    cin >> num;

    cout << "Enter digit to find frequency: ";
    cin >> target;

    while(num > 0) {
        digit = num % 10;
        if(digit == target)
            count++;
        num /= 10;
    }

    cout << "Frequency = " << count;

    return 0;
}
