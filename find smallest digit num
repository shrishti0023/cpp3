#include <iostream>
using namespace std;

int main() {
    int num, digit, smallest = 9;

    cout << "Enter number: ";
    cin >> num;

    while(num > 0) {
        digit = num % 10;
        if(digit < smallest)
            smallest = digit;
        num /= 10;
    }

    cout << "Smallest digit = " << smallest;

    return 0;
}
