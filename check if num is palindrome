#include <iostream>
using namespace std;

int main() {
    int num, original, reverse = 0, digit;
    cout << "Enter number: ";
    cin >> num;

    original = num;

    while(num > 0) {
        digit = num % 10;
        reverse = reverse * 10 + digit;
        num /= 10;
    }

    if(original == reverse)
        cout << "Palindrome number";
    else
        cout << "Not a palindrome";

    return 0;
}
